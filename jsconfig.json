{
"compilerOptions": {
  "module": "commonjs",
  "target": "es6",
  "baseUrl": ".",
  "paths": {
    "@/*": [
      "./src/*"
    ],
    "data/*": [
      "./src/data/*"
    ],
    "components/*": [
      "./src/components/*"
    ],
    "utils/*": [
      "./src/utils/*"
    ],
    "icons/*": [
      "./src/icons/*"
    ],
  }
},
"exclude": [
  "node_modules",
  "dist"
],
"include": [
  "src/**/*"
]
}
